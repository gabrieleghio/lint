{
  "devDependencies": {
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "typescript": "^5.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "frontend/**/*.ts": [
      "eslint --config frontend/.eslintrc --ext .ts",
      "tsc --project frontend/tsconfig.json --noEmit"
    ]
  }
}
